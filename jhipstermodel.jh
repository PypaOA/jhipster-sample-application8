
entity Cd2 {
	name String required,
    performer String,
    releaseYear String,
    discCount String,
    medium String,
    label String,
    state State,
    added Instant
}

entity Dvd2 {
name String required,
    performer String,
    releaseYear String,
    discCount String,
    format String,
    lang String,
    state State,
    added Instant
}

enum State {
OK, AWAY, LOST, EXPECTED, UNDEFINED
}

paginate Cd, Dvd with pagination 

service all with serviceImpl